  <script>
    function toggleLang(e){
      var currLang = e.getAttribute('lang');
      var langStateTransitions = {
        'en-us': 'zh-cn',
        'zh-cn': 'en-us'
      };
      i18n(langStateTransitions[currLang]);
    }
  </script>
